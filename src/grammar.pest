add = @{"+"}
sub = @{"-"}
mul = @{"*"}
div = @{"/"}
inp = @{"|"}
print = @{"$"}
debugprint = @{"#"}
dup = @{"."}
swp = @{":"}


float = @{ASCII_DIGIT+~"."~ASCII_DIGIT+}
int = @{ASCII_DIGIT+}
char = @{"'"~"\\"*~ANY}
operator = {add|sub|mul|div|inp|print|debugprint|dup|swp}
keyword = @{ASCII_ALPHA~ASCII_ALPHANUMERIC*}

token = {float|int|char|operator|keyword}